{
    "name": "zjkal/twig2html",
    "description": "A powerful tool to convert Twig templates to static HTML files",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "zjkal\\twig2html\\": "src/"
        }
    },
    "authors": [
        {
            "name": "zjkal",
            "email": "zjkal@qq.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-create-project-cmd": [
            "@init-project",
            "@build"
        ],
        "init-project": "php init.php",
        "build": "php build.php"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}